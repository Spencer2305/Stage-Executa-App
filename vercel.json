{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next@4.1.0"
    }
  ],
  "env": {
    "NEXT_TELEMETRY_DISABLED": "1"
  },
  "buildCommand": "npm run build",
  "installCommand": "npm ci",
  "framework": "nextjs",
  "functions": {
    "app/**": {
      "maxDuration": 30
    }
  }
} 